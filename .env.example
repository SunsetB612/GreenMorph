# GreenMorph 环境配置示例
# 复制此文件为 .env 并填入实际值

# MySQL 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USERNAME=root
MYSQL_PASSWORD=your-mysql-password
MYSQL_DATABASE=greenmorph

# JWT 配置
SECRET_KEY=your-secret-key-here-change-in-production

# AI 服务配置
TONGYI_BASE_URL=https://dashscope.aliyuncs.com/api/v1
TONGYI_MODEL=qwen-vl-plus
TONGYI_API_KEY=your-tongyi-api-key

ARK_API_KEY=your-ark-api-key

# 阿里云 OSS 配置
ALIYUN_OSS_ACCESS_KEY_ID=your-aliyun-access-key-id
ALIYUN_OSS_ACCESS_KEY_SECRET=your-aliyun-access-key-secret
ALIYUN_OSS_BUCKET_NAME=your-oss-bucket-name
ALIYUN_OSS_ENDPOINT=your-oss-endpoint

# SerpAPI 配置
SERPAPI_KEY=your-serpapi-key
