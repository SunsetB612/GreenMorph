# GreenMorph Environment Configuration Example
# Copy this file to .env and fill in actual configuration values

# Application Configuration
APP_NAME=GreenMorph API
APP_VERSION=1.0.0
DEBUG=false

# Server Configuration
HOST=0.0.0.0
PORT=8000

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads
OUTPUT_DIR=outputs

# Tongyi Qianwen Configuration
TONGYI_API_KEY=your_tongyi_api_key_here
TONGYI_MODEL=qwen-vl-plus
TONGYI_BASE_URL=https://dashscope.aliyuncs.com/api/v1

# OpenAI Configuration (Backup)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-vision-preview

# Anthropic Configuration (Backup)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-opus-20240229

# Replicate Configuration (Backup)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Image Generation Configuration
IMAGE_GENERATION_MODEL=stabilityai/stable-diffusion-xl-base-1.0
CONTROLNET_MODEL=lllyasviel/sd-controlnet-canny

# Image Processing Configuration
MAX_IMAGE_SIZE=[1024,1024]
IMAGE_QUALITY=95
